    <singleLineBlock name="string" foreground="DarkRed">
        <escape start="\"></escape>
        <start>"</start>
        <end>"</end>
    </singleLineBlock>

    <singleLineBlock name="singleLineComment" foreground="Green">
        <escape></escape>
        <start>//</start>
        <end></end>
    </singleLineBlock>

    <multiLinesBlock name="string" foreground="DarkRed">
        <escape>
            <item>""</item>
        </escape>
        <start>@"</start>
        <end>"</end>
    </multiLinesBlock>
    
    <multiLinesBlock name="multiLineComment" foreground="Gray">
        <escape></escape>
        <start>/*</start>
        <end>*/</end>
    </multiLinesBlock>

    <token pattern="#.*" name="precompile" foreground="Gray" />
    <token pattern="(0[x|X])?\d+" name="digit" foreground="Chocolate" />

